$('#ActivityFeed').append('<%= escape_javascript(render :partial => 'activity_feeds/activity_feed', :collection => @activity_feeds) %>');
<% if (@activity_feeds.current_page < @activity_feeds.num_pages) %>
        $('.pagination').replaceWith('<%= escape_javascript(render :partial => 'activity_feeds/activity_feed', :collection => @activity_feeds) %>');
<% else %>
        $('.pagination').remove();
<% end %>
<% sleep 1 %>




